<?xml version="1.0" encoding="UTF-8"?> 

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="150.0" prefWidth="270.0" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.exam.app.view.ReservationPrintPopupController">
   <children>
      <!-- 둥근 테두리를 위한 사각형 -->
      <Rectangle arcHeight="20.0" arcWidth="20.0" fill="WHITE" height="150.0" stroke="#b7b7b7" strokeWidth="1" width="270.0" />

      <!-- 텍스트 -->
      <Text fill="BLACK" layoutX="21.0" layoutY="56.0" strokeType="OUTSIDE" strokeWidth="0.0" text="[예매내역을 출력하겠습니까?]" textAlignment="CENTER" wrappingWidth="227.30596923828125" />

      <!-- 예/아니오 버튼 -->
      <Button fx:id="noButton" layoutX="154.0" layoutY="85.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="65.0" style="-fx-background-color: #ff5a5a; -fx-text-fill: white; -fx-background-radius: 20;" text="아니오" textAlignment="CENTER" onAction="#handleNoButtonAction"/>
      <Button fx:id="yesButton" layoutX="51.0" layoutY="85.0" mnemonicParsing="false" prefHeight="29.0" prefWidth="65.0" style="-fx-background-color: #ff5a5a; -fx-text-fill: white; -fx-background-radius: 20;" text="예" textAlignment="CENTER" onAction="#handleYesButtonAction"/>
   </children>
</AnchorPane>
